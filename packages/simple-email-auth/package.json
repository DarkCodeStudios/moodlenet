{
  "name": "@moodlenet/simple-email-auth",
  "version": "0.2.4",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": "16",
    "npm": "8",
    "yarn": "please-use-npm"
  },
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Email authentication"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Email authentication\nBasic authentication using email and password",
  "files": [
    "dist/",
    "src/"
  ],
  "exports": {
    "./init": "./dist/server/init.mjs",
    "./server": "./dist/server/exports.mjs",
    "./common": "./dist/common/exports.mjs",
    "./ui": "./dist/webapp/exports/ui.mjs",
    "./stories": "./dist/webapp/exports/stories.mjs",
    "./webapp": "./dist/webapp/exports/webapp.mjs"
  },
  "repository": "https://gitlab.com/moodlenet/moodlenet",
  "author": "https://github.com/aleclofabbro",
  "dependencies": {
    "@moodlenet/key-value-store": "^0.2.4",
    "dot": "1.1.3",
    "formik": "2.2.9",
    "yup": "0.32.11"
  },
  "peerDependencies": {
    "@moodlenet/arangodb": "^0.2.3",
    "@moodlenet/component-library": "^0.2.3",
    "@moodlenet/core": "^0.2.3",
    "@moodlenet/crypto": "^0.2.3",
    "@moodlenet/email-service": "^0.2.3",
    "@moodlenet/http-server": "^0.2.3",
    "@moodlenet/organization": "^0.2.3",
    "@moodlenet/react-app": "^0.2.3",
    "@moodlenet/web-user": "^0.2.3"
  },
  "devDependencies": {
    "@moodlenet/arangodb": "^0.2.4",
    "@moodlenet/component-library": "^0.2.4",
    "@moodlenet/core": "^0.2.4",
    "@moodlenet/crypto": "^0.2.4",
    "@moodlenet/email-service": "^0.2.4",
    "@moodlenet/http-server": "^0.2.4",
    "@moodlenet/organization": "^0.2.4",
    "@moodlenet/react-app": "^0.2.4",
    "@moodlenet/web-user": "^0.2.4",
    "@types/dot": "1.1.5",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "react-router-dom": "6.3.0",
    "typescript": "4.7.2"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-components-index",
    "dev-init": "npm run copy-assets",
    "prepublish": "npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json,jpg} dist",
    "create-components-index": "vgis src/webapp/ui.mts"
  },
  "watch": {
    "create-components-index": {
      "patterns": [
        "src/webapp/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  },
  "gitHead": "4b8737c02083f2eca71b432c601ba3f595cd4f51"
}
